{
  "name": "react-todo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cy:run": "cypress run",
    "cy:e2e": "cypress open --browser chrome --e2e",
    "cy:test-e2e": "start-server-and-test start http://localhost:5173 cy:run",
    "cy:open-e2e": "start-server-and-test start http://localhost:5173 cy:e2e",
    "cy:test-component": "cypress run --component",
    "cy:open-component": "cypress open --browser chrome --component"
  },
  "dependencies": {
    "antd": "^5.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}